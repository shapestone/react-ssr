    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.19.0",
    "express": "^4.16.4",
    "nodemon": "^1.18.11",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
